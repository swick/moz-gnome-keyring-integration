<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>gnome-keyring-integration@sebastianwick.net</em:id>
    <em:name>GNOME Keyring integration</em:name>
    <em:description>Store passwords and form logins in GNOME Keyring. Set keyring name in "extensions.gnome-keyring.keyringName" preference (about:config).</em:description>
    <em:version>0.10</em:version>
    <em:creator>Sebastian Wick</em:creator>
    <em:iconURL>chrome://gnome-keyring/skin/seahorse.svg</em:iconURL>
    <em:homepageURL>https://github.com/swick/mozilla-gnome-keyring</em:homepageURL>
    <em:type>2</em:type>

    <em:optionsURL>chrome://gnome-keyring/content/options.xul</em:optionsURL>

    <!-- Mozilla Firefox -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>4.0</em:minVersion>
        <em:maxVersion>50.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- Mozilla Thunderbird -->
    <em:targetApplication>
      <Description>
        <em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
        <em:minVersion>5.0</em:minVersion>
        <em:maxVersion>50.*</em:maxVersion>
      </Description>
    </em:targetApplication>
  </Description>
</RDF>
